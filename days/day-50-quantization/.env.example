# Day 50: Quantization - Environment Configuration Template
# Copy this file to .env and customize the values for your environment

# =============================================================================
# HARDWARE CONFIGURATION
# =============================================================================

# Target hardware for optimization (cpu, gpu, mobile)
TARGET_HARDWARE=cpu

# CPU optimization settings
CPU_THREADS=auto
ENABLE_MKLDNN=true
ENABLE_OPENMP=true

# GPU settings (if available)
CUDA_DEVICE=0
ENABLE_MIXED_PRECISION=true
CUDA_BENCHMARK=true

# Memory settings
MAX_MEMORY_GB=8
ENABLE_MEMORY_OPTIMIZATION=true

# =============================================================================
# QUANTIZATION CONFIGURATION
# =============================================================================

# Default quantization method (ptq, qat, gptq, awq)
DEFAULT_QUANTIZATION_METHOD=ptq

# Quantization precision (4, 8, 16)
QUANTIZATION_BITS=8

# Calibration settings
CALIBRATION_SAMPLES=1000
CALIBRATION_BATCH_SIZE=32

# GPTQ specific settings
GPTQ_GROUP_SIZE=128
GPTQ_BITS=4

# AWQ specific settings
AWQ_GROUP_SIZE=128
AWQ_BITS=4
AWQ_PROTECTION_RATIO=0.1

# =============================================================================
# MODEL CONFIGURATION
# =============================================================================

# Model paths
MODEL_PATH=./models/
QUANTIZED_MODEL_PATH=./models/quantized/
GGUF_MODEL_PATH=./models/gguf/

# Model settings
MODEL_NAME=test_model
MODEL_TYPE=cnn  # cnn, transformer, custom

# GGUF conversion settings
GGUF_QUANTIZATION_TYPE=q4_0  # f32, f16, q8_0, q4_0, q4_1, q5_0, q5_1, q2_k, q3_k, q4_k, q5_k, q6_k, q8_k

# =============================================================================
# PERFORMANCE MONITORING
# =============================================================================

# Monitoring settings
ENABLE_MONITORING=true
MONITORING_FREQUENCY=real_time  # real_time, batch, periodic
METRICS_RETENTION_HOURS=24

# Performance thresholds
MAX_LATENCY_MS=100
MIN_ACCURACY=0.85
MAX_ERROR_RATE=0.05
MAX_MEMORY_MB=500

# Alert settings
ENABLE_ALERTS=true
ALERT_EMAIL=admin@example.com
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK

# =============================================================================
# BENCHMARKING CONFIGURATION
# =============================================================================

# Benchmark settings
BENCHMARK_RUNS=100
WARMUP_RUNS=10
BENCHMARK_BATCH_SIZE=1

# Input data settings
INPUT_HEIGHT=224
INPUT_WIDTH=224
INPUT_CHANNELS=3

# =============================================================================
# DEPLOYMENT CONFIGURATION
# =============================================================================

# Deployment settings
DEPLOYMENT_MODE=local  # local, cloud, edge
ENABLE_VALIDATION=true
VALIDATION_SAMPLES=100

# API settings (if deploying as service)
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=1

# Model serving settings
BATCH_SIZE=1
MAX_BATCH_DELAY_MS=10
ENABLE_BATCHING=false

# =============================================================================
# LOGGING AND DEBUGGING
# =============================================================================

# Logging configuration
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_FILE=./logs/quantization.log
ENABLE_FILE_LOGGING=true
ENABLE_CONSOLE_LOGGING=true

# Debug settings
DEBUG_MODE=false
ENABLE_PROFILING=false
PROFILE_OUTPUT_DIR=./profiles/

# Verbose output
VERBOSE_QUANTIZATION=false
VERBOSE_BENCHMARKING=false
VERBOSE_MONITORING=false

# =============================================================================
# CLOUD AND STORAGE CONFIGURATION
# =============================================================================

# AWS settings (if using cloud storage)
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-east-1
S3_BUCKET=your-model-bucket

# Google Cloud settings
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_BUCKET=your-model-bucket

# Azure settings
AZURE_STORAGE_ACCOUNT=your_storage_account
AZURE_STORAGE_KEY=your_storage_key
AZURE_CONTAINER=your-container

# =============================================================================
# EXPERIMENTAL FEATURES
# =============================================================================

# Experimental quantization methods
ENABLE_EXPERIMENTAL_QUANTIZATION=false
EXPERIMENTAL_BITS=2

# Advanced optimizations
ENABLE_GRAPH_OPTIMIZATION=false
ENABLE_KERNEL_FUSION=false
ENABLE_MEMORY_PLANNING=false

# Research features
ENABLE_QUANTIZATION_RESEARCH_MODE=false
RESEARCH_OUTPUT_DIR=./research/

# =============================================================================
# SECURITY AND PRIVACY
# =============================================================================

# Security settings
ENABLE_MODEL_ENCRYPTION=false
ENCRYPTION_KEY=your_encryption_key

# Privacy settings
ANONYMIZE_LOGS=true
ENABLE_DIFFERENTIAL_PRIVACY=false

# =============================================================================
# TESTING AND VALIDATION
# =============================================================================

# Test configuration
RUN_TESTS_ON_STARTUP=false
TEST_DATA_PATH=./test_data/
ENABLE_ACCURACY_TESTS=true
ENABLE_PERFORMANCE_TESTS=true

# Validation settings
VALIDATION_SPLIT=0.1
VALIDATION_SEED=42
CROSS_VALIDATION_FOLDS=5

# =============================================================================
# INTEGRATION SETTINGS
# =============================================================================

# MLflow integration
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_EXPERIMENT_NAME=quantization_experiments
ENABLE_MLFLOW_LOGGING=false

# Weights & Biases integration
WANDB_PROJECT=quantization-optimization
WANDB_ENTITY=your_entity
ENABLE_WANDB_LOGGING=false

# TensorBoard integration
TENSORBOARD_LOG_DIR=./tensorboard_logs/
ENABLE_TENSORBOARD=false

# =============================================================================
# CUSTOM CONFIGURATION
# =============================================================================

# Custom model configurations
CUSTOM_MODEL_CONFIG={}
CUSTOM_QUANTIZATION_CONFIG={}
CUSTOM_DEPLOYMENT_CONFIG={}

# Feature flags
ENABLE_FEATURE_A=false
ENABLE_FEATURE_B=false
ENABLE_FEATURE_C=false

# Environment-specific overrides
ENVIRONMENT=development  # development, staging, production
CONFIG_OVERRIDE_FILE=./config/override.yaml